
<table class="table table-bordered">
	<tbody>
				<tr class="well">
			<td colspan="2"><i class="icon-signal"></i> <b>Statistics</b></td>
		</tr>
		<tr >
			<td>Chromosomes</td>
			<td><%= (counter.attr('complete.chromosomes')* 1).toLocaleString() %></td>
		</tr>
		<tr >
			<td>Genomes</td>
			<td><%= Math.round(counter.attr('complete.chromosomes') / 22).toLocaleString() %></td>
		</tr>
	</tbody>
</table>
<br>
<table class="table table-bordered">
	<tbody>
		<tr class="well">
			<td colspan="2"><i class="icon-signal"></i> <b>Server Status</b></td>
		</tr>
		<tr >
			<td colspan="2"><b><%= counter.attr('running.chromosomes')? (counter.attr('running.chromosomes')* 1).toLocaleString() : '0' %></b> Chromosomes are currently in progress.</td>
		</tr>
		<tr >
			<td colspan="2"><b><%= counter.attr('waiting.chromosomes')? (counter.attr('waiting.chromosomes')* 1).toLocaleString() : '0' %></b> Chromosomes are in the queue.</td>
		</tr>		
	</tbody>
</table>